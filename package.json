{
  "name": "FeathersNuxt",
  "description": "FeathersNuxt starter template",
  "version": "0.0.0",
  "homepage": "",
  "main": "server",
  "keywords": [
    "feathers",
    "vuejs",
    "nuxt"
  ],
  "author": {
    "name": "Brian Kilgore",
    "email": "brianleekilgore@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server",
    "test": "test/"
  },
  "engines": {
    "node": "8.1.1",
    "npm": ">= 3.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint server/. test/. --config .eslintrc.json",
    "precommit": "npm run eslint",
    "build": "nuxt build",
    "dev": "DEBUG=nuxt:* nodemon --watch server/ --watch config/ server/index.js",
    "dev-debug": "DEBUG=nuxt:* nodemon --inspect-brk --watch server/ --watch config/ server/index.js",
    "start": "NODE_ENV=production node server/index.js",
    "mocha": "mocha test/ --recursive",
    "heroku-postbuild": "nuxt build"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.1",
    "@feathersjs/authentication-client": "^1.0.2",
