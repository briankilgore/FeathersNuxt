    "babel-eslint": "^7.2.3",
    "css-loader": "^0.28.9",
    "dotenv": "^4.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.1",
