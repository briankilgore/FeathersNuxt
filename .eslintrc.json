{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "extends": "standard",
  "plugins": [
    "html"
  ],
  "rules": {
    "import/extensions": ["off", "never", {
      "js": "never",
      "vue": "never"
    }],
    "import/no-extraneous-dependencies": 0,
    "func-names": 0,
    "comma-dangle": [2, "always-multiline"],
    "semi": [2, "always"],
    "space-before-function-paren": [2, "always"]
  }
}
